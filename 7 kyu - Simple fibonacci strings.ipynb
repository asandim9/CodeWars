    "def solve(n):\n",
    "    a1 = [\"0\"]\n",
    "    a2 = [\"01\"]\n",
    "    a = []\n",
    "    for i in range(0,n+1):\n",
    "        if(i == 0):\n",
    "            a = a1\n",
    "        elif(i==1):\n",
    "            a = a2\n",
    "        else:\n",
    "            a = a2 + a1\n",
    "            a1 = a2\n",
    "            a2 = a\n",
    "        \n",
    "    return (''.join(map(str,a)))"
   ]
  }
